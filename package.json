    "generate-changelog": "^1.4.0",
    "iconv-lite": "^0.4.24",
    "install": "^0.12.2",
    "jest": "^23.6.0",
    "jquery": "^3.5.1",
    "jsdoc": "^3.6.5",
    "lodash": "^4.17.19",
    "regenerator-runtime": "^0.12.1",
    "sinon": "^3.3.0"
  },
